type User {
  id: ID!
  name: String!
  username: String!
  email: String!
  password: String!
  enabled: Boolean!
  isUsing2FA: Boolean!
  secret: String!

  roles: [String!]!
}

input CreateUserInput {
  name: String!
  username: String!
  email: String!
  password: String!
  matchingPassword: String!
}

input UserInput {
  id: ID!
  name: String!
  username: String!
  email: String!
}

