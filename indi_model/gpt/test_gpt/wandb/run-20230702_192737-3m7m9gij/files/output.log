        return *this;
    }
    A&amp; operator++(int) {
        cout &lt;&lt; c_ &lt;&lt; "B ";
        return *this;
    }
    A&amp; operator--() {
        cout &lt;&lt; c_ &lt;&lt; "C ";
        return *this;
    }
    A&amp; operator--(int) {
        cout &lt;&lt; c_ &lt;&lt; "D ";
        return *this;
    }
    void operator+(A&amp; b) {
        cout &lt;&lt; c_ &lt;&lt; "U ";
